<h1>{{title}}</h1>
<p style="color: aliceblue; font-size:200%">Ucuz Al'a Ho≈ügeldiniz... </p>
<p>merhaba{{session.username.username}}</p>


{{# each products }}
  <div class="row">
    <div class="col-md-12">
      <div class="thumbnail">
        <div class="caption" style="background-color: pink;">
          <h3>{{ this.title }}</h3>
            <img src={{this.image}} alt={{ this.title }} width="400" height="200">
            <p class="description">{{{this.description}}}</p>
            <div class="clearfix">
              <a href="{{ this.url }}" class="btn btn-default pull-right" role="button"><i class="fa fa-paper-plane" aria-hidden="true"></i> Sayfaya Git</a>
            </div>
        </div>
      </div>
    </div>
  </div>
{{/each}}